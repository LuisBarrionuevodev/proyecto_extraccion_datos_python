    "import requests\n",
    "from bs4 import BeautifulSoup   \n",
    "import pandas as pd\n",
    "import time\n",
    "url_base = \"https://books.toscrape.com/catalogue/page-{}.html\"\n",
    "product_list=[]\n",
    "\n",
    "\n",
    "for page in range(1,2):\n",
    "    url_formateado = url_base.format(page)   \n",
    "    try:\n",
    "        response = requests.get(url_formateado)\n",
    "        response.raise_for_status()\n",
    "        soup = BeautifulSoup(response.text,\"html.parser\")\n",
    "        productos = soup.select(\"article.product_pod\")\n",
    "    except requests.RequestException as e:\n",
    "        print(f\"Error en la pagina{page}: {e}\")\n",
    "        continue\n",
    "    for producto in productos:\n",
    "        \n",
    "        try :\n",
    "            nombre = producto.find(\"h3\").find(\"a\")[\"title\"]\n",
    "            precio = producto.find(\"div\", class_=\"product_price\").find(\"p\").get_text(strip = True)\n",
    "            imagen = producto.find(\"div\").find(\"img\")[\"src\"]\n",
    "            imagen_url= url_formateado + imagen  \n",
    "            product_list.append({\n",
    "            \"nombre\": nombre,\n",
    "            \"precio\": precio,\n",
    "            \"imagen_url\": imagen_url\n",
    "            })\n",
    "        except Exception as e:\n",
    "            print (\"Error al extraer un producto\",e)\n",
    "                \n",
    "    time.sleep(2)\n",
    "    print(f\"Pagina {page} procesada\")           \n",
    "    \n",
    "    "
   ]
